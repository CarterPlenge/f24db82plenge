extends layout

block content
  h1= title
  
  - var image_names = ['image1.png', 'image2.png', 'image3.png', 'image4.png', 'image5.png']
  - for (let i = 0; i < 3; i++)
    - let randomIndex = Math.floor(Math.random() * 5)
    - let imageName = image_names[randomIndex]
    img(src=`/images/${imageName}`, alt=`Random image ${i + 1}`, height="300", width="300")
